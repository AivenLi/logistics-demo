<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    >
    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已签收"
        android:textSize="@dimen/jingdong_title"
        android:textColor="@color/color_jingdong_selected_title"
        android:textStyle="bold"
        android:layout_marginBottom="6dp"
        />
    <TextView
        android:id="@+id/tv_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="到就发生了的房价松动"
        android:textSize="@dimen/jingdong_normal_desc"
        android:textColor="@color/color_jingdong_normal"
        />
    <TextView
        android:id="@+id/tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2022-12-12 00:00:00"
        android:textSize="@dimen/jingdong_selected_desc"
        android:textStyle="bold"
        android:layout_marginTop="6dp"
        />
</LinearLayout>